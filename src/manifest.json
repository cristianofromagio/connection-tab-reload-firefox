{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.0",
  "default_locale": "en",
  "homepage_url": "https://github.com/cristianofromagio/connection-tab-reload-firefox",
  "icons": {
    "48": "icons/icon-online-96.svg",
    "96": "icons/icon-online-96.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-online-96.svg",
    "default_title": "__MSG_extensionTitle__"
  },

  "background": {
    "scripts": [
      "vendor/browser-polyfill.min.js",
      "background.js"
    ]
  }

}
